riginal":{"line":52,"column":1}},{"name":"ConductTransaction","generated":{"line":116,"column":15},"source":"ConductTransaction.js","original":{"line":54,"column":15}}],"sources":{"ConductTransaction.js":"import React,{Component} from 'react';\nimport {FormGroup,FormControl,Button} from 'react-bootstrap';\nimport {Link} from \"react-router-dom\";\n\nclass ConductTransaction extends Component{\n    state = {recipient:'',amount:0};\n\n    updateRecipient = (event) =>{\n        this.setState({recipient : event.target.value});\n    }\n\n    updateAmount = (event) =>{\n        this.setState({amount : Number(event.target.value)});\n    }\n\n    conductTransaction = () =>{\n        const {recipient,amount} = this.state;\n\n        fetch(\"http://localhost:3000/api/transact\",{\n            method:'POST',\n            headers:{'Content-Type':'application/json'},\n            body: JSON.stringify({recipient,amount})\n        }).then((response)=>response.json())\n          .then(responseJson =>{\n            alert(responseJson.message || responseJson.type);\n          });\n    }\n\n    render() {\n        return(\n            <div className=\"ConductTransaction\">\n                <Link to=\"/\">Home</Link>\n                <h3>Conduct a Transaction</h3>\n                <FormGroup>\n                    <FormControl \n                     input=\"text\"\n                     placeholder=\"Recipient\"\n                     value={this.state.recipient}\n                     onChange={this.updateRecipient}/>\n                </FormGroup>\n                <FormGroup>\n                    <FormControl \n                     input=\"number\"\n                     placeholder=\"Amount\"\n                     value={this.state.amount}\n                     onChange={this.updateAmount}/>\n                </FormGroup>\n                <Button variant=\"danger\" onClick={this.conductTransaction}>Submit</Button>\n            </div>\n        );\n    }\n};\n\nexport default ConductTransaction;"},"lineCount":null}},"error":null,"hash":"69ca2d0459db64833c1add3b93c3534e","cacheData":{"env":{}}}